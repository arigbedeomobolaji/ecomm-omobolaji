{
	"name": "ecomm",
	"type": "module",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"engines": {
		"node": "18.x",
		"npm": "9.x"
	},
	"scripts": {
		"client": "npm run start --prefix client",
		"server:dev": "nodemon --exec node --experimental-modules server.js",
		"server:prod": "cross-env NODE_ENV=production nodemon --exec node --experimental-modules server.js",
		"dev": "concurrently \"npm run server:dev\" \"npm run client\"",
		"prod": "concurrently \"npm run server:prod\" \"npm run client\"",
		"start": "node --experimental-modules server.js",
		"build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
		"heroku-postbuild": "npm run build"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"aws-sdk": "^2.1454.0",
		"bcryptjs": "^2.4.3",
		"concurrently": "^8.2.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^7.5.0",
		"nodemon": "^3.0.1",
		"react-router-dom": "^5.2.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"eslint": "^7.24.0",
		"eslint-plugin-react": "^7.23.2"
	}
}
